        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "import flet as ft\n\ndef main(page: ft.Page):\n    def change_text(e):\n        tx.value = \"Vertical\"\n        print(\"内容を更新しました\")\n\n    tx = ft.Text(\n        value=\"Horizon\",\n        size=50\n    )\n\n    bt = ft.ElevatedButton(\n        text=\"テキストの内容を更新\",\n        on_click=change_text\n    )\n\n    page.add(tx, bt)\n\nft.app(target=main)",
          "href": null
        }
      ]
    },
    "order": 3
  },
