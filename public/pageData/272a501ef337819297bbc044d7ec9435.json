  {
    "id": "272a501ef33781a6b9d8c9a115294574",
    "type": "code",
    "parentId": "272a501ef3378117be76cd3e3cdbe5b2",
    "curriculumId": "272a501ef3378040af9bfae333f7427a",
    "pageId": "272a501ef337819297bbc044d7ec9435",
    "data": {
      "caption": [],
      "language": "python",
      "parent": [
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "import flet as ft\n\ndef main(page: ft.Page):\n    appbar = ft.AppBar(\n        center_title=True,\n        title=ft.Text(value=\"TODOアプリ\"),\n        bgcolor=ft.Colors.BLACK12\n    )\n\n    def route_change(e):\n        template_route = ft.TemplateRoute(page.route)\n\n        if template_route.match(\"/create\"):\n            appbar.leading = ft.IconButton(icon=ft.Icons.ARROW_BACK, on_click=lambda _:page.go(\"/\"))\n            page.views.append(\n                ft.View(\n                    \"/create\",\n                    appbar=appbar,\n                    controls=[]\n                )\n            )\n        elif template_route.match(\"/\"):\n            appbar.leading = ft.Text(\"\")\n            floating_button = ft.FloatingActionButton(\n                icon=ft.Icons.ADD,\n                bgcolor=ft.Colors.BLUE_200,\n                on_click=lambda _: page.go(\"/create\")\n            )\n            page.views.append(\n                ft.View(\n                    \"/\",\n                    appbar=appbar,\n                    controls=[],\n                    floating_action_button=floating_button\n                )\n            )\n        page.update()\n\n    page.on_route_change = route_change\n    page.go(page.route)\n\nft.app(main)\n",
          "href": null
        }
      ]
    },
    "order": 0
  },
