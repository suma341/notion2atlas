  {
    "id": "1eca501ef33781acbc9ed9216b968340",
    "type": "paragraph",
    "parentId": "1eca501ef33781e2bb62d04269f9236f",
    "curriculumId": "1e7a501ef337805bbc4ed442525244f3",
    "pageId": "1eca501ef33781e2bb62d04269f9236f",
    "data": {
      "color": "default",
      "parent": [
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "前回と同様ですが、ここで必要となる新たな知識はありません。少しヒントを添えると、",
          "href": null
        },
        {
          "annotations": {
            "bold": true,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": true
          },
          "plain_text": "どこからどこまでを1回と数えるか",
          "href": null
        },
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "、",
          "href": null
        },
        {
          "annotations": {
            "bold": true,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": true
          },
          "plain_text": "そしてその回数をどこにおいておけばいいのか",
          "href": null
        },
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "ということを考えてみてみましょう。",
          "href": null
        }
      ]
    },
    "order": 7
  },
