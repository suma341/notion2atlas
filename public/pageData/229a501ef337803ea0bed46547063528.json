  {
    "id": "229a501ef33781ca8630dff7aede9f64",
    "type": "code",
    "parentId": "229a501ef337803ea0bed46547063528",
    "curriculumId": "1f5a501ef3378055b23cf8b1b79b780a",
    "pageId": "229a501ef337803ea0bed46547063528",
    "data": {
      "caption": [],
      "language": "python",
      "parent": [
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "import flet as ft\n\ndef main(page: ft.Page):\n    def plus(e):\n        txt.value = txt.value + 1\n        txt.update()\n    \n    def minus(e):\n        txt.value = txt.value - 1\n        txt.update()\n    \n    txt = ft.Text(value=\"0\")\n    plus_button = ft.ElevatedButton(text=\"プラス\", on_click=plus)\n    minus_button = ft.ElevatedButton(text=\"マイナス\", on_click=minus)\n\n    page.add(txt, plus_button, minus_button)\n\nft.app(target=main)\n",
          "href": null
        }
      ]
    },
    "order": 39
