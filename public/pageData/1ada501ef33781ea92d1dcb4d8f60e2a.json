      "is_toggleable": false,
      "parent": [
        {
          "annotations": {
            "bold": false,
            "code": false,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "VSCodeで拡張機能をインストールする",
          "href": null
        }
      ]
    },
    "order": 7
  },
  {
    "id": "1ada501ef3378123b487e1dc5e249f46",
    "type": "divider",
    "parentId": "1ada501ef33781ea92d1dcb4d8f60e2a",
    "curriculumId": "1ada501ef33781e8b251df0f38a0cb23",
