            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "クラスや",
          "href": null
        },
        {
          "annotations": {
            "bold": false,
            "code": true,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "goblin",
