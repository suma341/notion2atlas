            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "となると、",
          "href": null
        },
        {
          "annotations": {
            "bold": false,
            "code": true,
            "color": "default",
            "italic": false,
            "strikethrough": false,
            "underline": false
          },
          "plain_text": "enemy",
          "href": null
